{
  "name": "travel-server",
  "version": "1.0.0",
  "description": "\"server for travel-agency app\"",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-assets",
    "dev:start": "npm-run-all clean tsc start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "client": "npm run start --prefix ../travel-client",
    "cs": "concurrently \"npm run dev\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yana Karcheuskaya",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/node-fetch": "^2.5.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-async-await": "^1.1.0",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongodb": "^3.5.2",
    "mongoose": "^5.8.10",
    "multer": "^1.4.2",
    "node-fetch": "^3.1.1",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-vkontakte": "^0.3.2",
    "session-file-store": "^1.4.0",
    "xlsx": "^0.15.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mongodb": "^3.3.15",
    "@types/mongoose": "^5.5.43",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.5.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-vkontakte": "^1.3.0",
    "@types/shelljs": "^0.8.6",
    "concurrently": "^5.1.0",
    "fs-extra": "^8.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
